{
  "compilerOptions": {
    "rootDir": ".",
    "moduleResolution": "node",
    "baseUrl": "./",
    "jsx": "react-jsx",
    "target": "ES5", // 编译成什么版本
    "module": "ESNext", // 当前代码语法
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts"],
  "exclude": ["dist"]
}
